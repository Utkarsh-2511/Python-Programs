    "def findx(s,x,si,ei):\n",
    "    l=len(s)\n",
    "    if si>ei:\n",
    "        return -1\n",
    "    mid = (si+ei)//2\n",
    "    if s[mid]==x:\n",
    "        return mid\n",
    "    elif s[mid]>x:\n",
    "        return findx(s,x,si,mid-1)\n",
    "    else:\n",
    "        return findx(s,x,mid+1,ei)"
   ]
